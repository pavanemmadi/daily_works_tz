package assinments;

import java.util.Scanner;

public class Reverse4Num {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("enter the 4 numbers ;");
		int n1 = sc.nextInt();
		int n2 = sc.nextInt();
		int n3 = sc.nextInt();
		int n4 = sc.nextInt();
		System.out.println(n4 + n3 + n2);
		
		

	}

}
