
public class NestedIf {

	public static void main(String[] args) {
		String name ="pavan";
		String location  = "hyd";
		String phone = "9553854482";
		
		if(name =="suresh") {
			if(location == "hyd") {
				if(phone =="9553854482")
					System.out.println("you are pavan");
				
			}
			else
			{ System.out.println("you are not pavan");
		}

	}

}
}